     pedidos.`cod_ped` AS pedidos_cod_ped,
     pedidos.`cod_prov` AS pedidos_cod_prov,
     pedidos.`cod_usu` AS pedidos_cod_usu,
     pedidos.`fecha_ped` AS pedidos_fecha_ped,
