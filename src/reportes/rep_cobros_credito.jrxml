	<parameter name="cod_cred" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     creditos.`cod_vntas` AS creditos_cod_vntas,
     creditos.`montoTotal` AS creditos_montoTotal,
     creditos.`montoinicial` AS creditos_montoinicial,
     creditos.`cant_cuotas` AS creditos_cant_cuotas,
     ventas.`cod_vntas` AS ventas_cod_vntas,
     ventas.`date` AS ventas_date,
     ventas.`sub_total` AS ventas_sub_total,
     ventas.`iva_vntas` AS ventas_iva_vntas,
     ventas.`descuento` AS ventas_descuento,
     ventas.`total_vntas` AS ventas_total_vntas,
     personal.`cod_usu` AS personal_cod_usu,
     personal.`nom_usu` AS personal_nom_usu,
     personal.`ape_usu` AS personal_ape_usu,
     cliente_A.`ced_cli` AS cliente_A_ced_cli,
     cliente_A.`nom_cli` AS cliente_A_nom_cli,
     cliente_A.`ape_cli` AS cliente_A_ape_cli,
     cliente_A.`dir_cli` AS cliente_A_dir_cli,
     cliente_A.`telf_cli` AS cliente_A_telf_cli,
     cliente_A.`email_cli` AS cliente_A_email_cli,
     ciudad.`nom_ciu` AS ciudad_nom_ciu,
     det_creditos.`id_cred` AS det_creditos_id_cred,
     det_creditos.`importe` AS det_creditos_importe,
     det_creditos.`date` AS det_creditos_date,
     det_creditos.`descrip` AS det_creditos_descrip,
     empresa.`cod_empresa` AS empresa_cod_empresa,
     empresa.`nom_empresa` AS empresa_nom_empresa,
     empresa.`ruc` AS empresa_ruc,
     empresa.`nom_representante` AS empresa_nom_representante,
     empresa.`telf` AS empresa_telf,
     empresa.`direccion` AS empresa_direccion,
     empresa.`email` AS empresa_email,
     empresa.`estado` AS empresa_estado,
     creditos.`id_cred` AS creditos_id_cred,
     det_creditos.`id_det_cred` AS det_creditos_id_det_cred
FROM
     `ventas` ventas INNER JOIN `creditos` creditos ON ventas.`cod_vntas` = creditos.`cod_vntas`
     INNER JOIN `personal` personal ON ventas.`cod_usu` = personal.`cod_usu`
     INNER JOIN `cliente` cliente_A ON ventas.`cod_cli` = cliente_A.`cod_cli`
     AND cliente_A.`cod_cli` = creditos.`id_cli`
     INNER JOIN `ciudad` ciudad ON cliente_A.`cod_ciu` = ciudad.`cod_ciu`
     INNER JOIN `det_creditos` det_creditos ON creditos.`id_cred` = det_creditos.`id_cred`,
     `empresa` empresa
WHERE
     det_creditos.id_det_cred = $P!{cod_cred} and empresa.estado=1]]>
	</queryString>
